#include<AT89S52.h>
const char b[10]={0xC0,0xF9,0xA4,0xB0,0x99,0x92,0x82,0xF8,0x80,0x90};
void wait(int);
void main(){
  char i;
   P0=0xFF;
   while(1){
    for(i=0;i<10;i++){
       P0=b[i];
       wait(5);
      } 
   }
}
void wait(int del){
    int i,j;
    for(i=0;i<del;i++){
       for(j=0;j<10;j++){
       }
      } 
  }
